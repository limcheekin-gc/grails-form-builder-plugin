<plugin name='form-builder' version='0.1' grailsVersion='1.3.6 &gt; *'>
  <author>Lim Chee Kin</author>
  <authorEmail>limcheekin@vobject.com</authorEmail>
  <title>Grails Form Builder Plugin - Create Online Forms without Coding</title>
  <description> The Grails Form Builder Plugin supports user to create online forms in web browser 
 without any programming knowledge.
 
 * Project Site and Documentation: http://code.google.com/p/grails-form-builder-plugin/
 * Support: http://code.google.com/p/grails-form-builder-plugin/issues/list
 * Discussion Forum: http://groups.google.com/group/grails-form-builder-plugin
</description>
  <documentation>http://grails.org/plugin/form-builder</documentation>
  <resources>
    <resource>BuildConfig</resource>
    <resource>Config</resource>
    <resource>DataSource</resource>
    <resource>UrlMappings</resource>
    <resource>org.grails.formbuilder.FormController</resource>
    <resource>org.grails.formbuilder.FormViewerController</resource>
    <resource>org.codehaus.groovy.grails.plugins.freemarker.FreeMarkerTemplate</resource>
    <resource>org.grails.formbuilder.DomainClass</resource>
    <resource>org.grails.formbuilder.Field</resource>
    <resource>org.grails.formbuilder.Form</resource>
    <resource>org.grails.formbuilder.DomainClassService</resource>
    <resource>org.grails.formbuilder.FormTemplateService</resource>
    <resource>org.grails.formbuilder.FormViewerTemplateService</resource>
  </resources>
  <dependencies>
    <plugin name='jqueryValidationUi' version='1.1.1 &gt; *' />
    <plugin name='jqueryUi' version='1.8.6 &gt; *' />
    <plugin name='jqueryJson' version='2.2 &gt; *' />
    <plugin name='freemarkerTags' version='0.5.8 &gt; *' />
    <plugin name='uniForm' version='1.5 &gt; *' />
    <plugin name='jqueryDatatables' version='1.7.5 &gt; *' />
  </dependencies>
  <behavior />
</plugin>